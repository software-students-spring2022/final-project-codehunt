{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "google"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": "off",
    "require-jsdoc": "off",
    "semi": ["error", "never"],
    "quotes": ["error", "double", { "avoidEscape": true }]
  }
}
